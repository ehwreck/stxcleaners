{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-unused-expressions": "warn",
      "no-unused-labels": "warn",
      "max-len": "warn"
    }
}
